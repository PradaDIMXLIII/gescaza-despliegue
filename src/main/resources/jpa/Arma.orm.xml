<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
			     xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
			     xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm 
			     				     http://java.sun.com/xml/ns/persistence/orm_1_0.xsd"
			     version="1.0">

    <mapped-superclass class="es.prada.cazador.Arma" access="FIELD">
        <attributes>
            <basic name="guia" optional="false"></basic>
            <basic name="calibre" optional="false">
                <column length="32"/>
            </basic>
            <basic name="nombre" optional="false">
                <column length="32"/>
            </basic>
            <basic name="descripcion" optional="false"></basic>
        </attributes>
     </mapped-superclass>
</entity-mappings>