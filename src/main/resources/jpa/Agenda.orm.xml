<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm 
        http://java.sun.com/xml/ns/persistence/orm_1_0.xsd"
    version="1.0">

    <mapped-superclass class="es.prada.eventos.Agenda" access="FIELD">
        <attributes>        	
            <basic name="anyo" optional="false">
            	<column length="32"/>
            </basic>
            <basic name="nombre" optional="false">
            	<column length="32"/>
            </basic>
            <!-- <transient name="eventos"/> -->
            <one-to-many name="eventos" target-entity="es.mde.gescaza.entidades.EventoConId" mapped-by="agenda">
            </one-to-many>
        </attributes>
    </mapped-superclass>

</entity-mappings>